@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
    var title = ViewData["Name"];
    List<QuickType.Specimen> allSpecimens = (List<QuickType.Specimen>) ViewData["allSpecimens"];
}

<div class="text-center">
    <h1 class="display-4">Welcome @title</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
    <table>
        <tr>
            <th>Plant ID</th>
            <th>Specimen ID</th>
            <th>Latitude</th>
            <th>Longitude</th>
            <th>Name</th>
            <th>Address</th>
        </tr>
        @{
            foreach (QuickType.Specimen specimen in allSpecimens)
            {
        <tr>
            <td>@specimen.PlantId</td>
            <td>@specimen.SpecimenId</td>
            <td>@specimen.Lat</td>
            <td>@specimen.Lng</td>
            <td>@specimen.Genus @specimen.Species @specimen.Common</td>
            <td>@specimen.Address</td>
        </tr>
            }
        }
    </table>
</div>
